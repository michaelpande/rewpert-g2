  <conceptSet>
    <concept id="scn010100" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010100" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Portrait</name>
      
      <definition xml:lang="de">Ansicht nur des Kopfes einer oder mehrerer Personen oder von einem oder mehreren Tieren.</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010200" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010200" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Halbfigur</name>
      
      <definition xml:lang="de">Ansicht des Oberkörpers einer oder mehrerer Personen</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010300" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010300" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Ganzfigur</name>
      
      <definition xml:lang="de">Ansicht der vollständigen Figur einer oder mehrerer Personen</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010400" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010400" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Profilansicht</name>
      
      <definition xml:lang="de">Ansicht einer oder mehrerer Personen von der Seite</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010500" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010500" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Rückansicht</name>
      
      <definition xml:lang="de">Ansicht einer oder mehrerer Personen von hinten</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010600" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010600" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Einzelperson</name>
      
      <definition xml:lang="de">Ansicht einer einzelnen Person</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010700" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010700" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Paar</name>
      <definition xml:lang="de">Ansicht von zwei Personen zwischen denen eine persönliche Beziehung besteht, z.B. sind sie verlobt, verheiratet oder einer sonstigen Liebesbeziehung</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010800" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010800" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Zwei Personen</name>
      
      <definition xml:lang="de">Ansicht von zwei Personen</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn010900" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:010900" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Gruppe</name>
      <definition xml:lang="de">Ansicht von mehr als zwei Personen</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011000" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011000" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Gesamtansicht</name>
      
      
      <definition xml:lang="de">Ansicht eines Subjekts und von dessen Umgebung</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011100" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011100" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Panorama</name>
      
      
      <definition xml:lang="de">Ein Panoramaansicht oder Weitwinkelansicht eines Subjekts und von dessen Umgebung</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011200" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011200" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Luftbild</name>
      <definition xml:lang="de">Eine Ansicht aus der Höhe</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011300" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011300" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Unter Wasser</name>
      
      <definition xml:lang="de">Ein unter Wasser aufgenommenes Bild</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011400" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011400" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Nachtaufnahme</name>
      <definition xml:lang="de">Ein Bild das während der Dunkelheit aufgenommen wurde</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011500" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011500" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Satellitenansicht</name>
      
      
      <definition xml:lang="de">Ein Bild das aus einer Satellitenumlaufbahn aufgenommen wurde</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011600" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011600" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Außenansicht</name>
      
      
      <definition xml:lang="de">Ein Bild das außerhalb  eines Gebäudes oder ähnlichen Objekts aufgenommen wurde.</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011700" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011700" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Innenansicht</name>
      <definition xml:lang="de">Ein Bild das innerhalb eines Gebäudes oder ähnlichen Objekts aufgenommen wurde</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011800" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011800" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Nahaufnahme</name>
      <definition xml:lang="de">Eine Ansicht von einer Person oder einem Objekt das aus nächster Nähe um Details oder Emotionen hervorzustreichen. Auch Makro-Fotografie.</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn011900" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:011900" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Aktionsaufnahme</name>
      
      
      <definition xml:lang="de">Subjekt in Bewegung, wie hüpfendes Kind, laufendes Pferd usw.</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn012000" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:012000" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Beim Auftritt</name>
      
      <definition xml:lang="de">Subjekt(e) auf einer Bühne, während sie vor Publikum auftreten</definition>
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn012100" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:012100" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Posieren</name>
      <definition xml:lang="de">Subjekt(e) die eine besondere Körperposition einnehmen, wie ein "Victory"-Zeichen</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn012200" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:012200" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Symbol</name>
      <definition xml:lang="de">Ein gestaltetes Bild das ein Ereignis symbolisiert, wie zwei Ringe für die Ehe</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn012300" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:012300" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      <name xml:lang="de">Ausgefallenes</name>
      
      <definition xml:lang="de">Ein attraktives, vielleicht auch lustiges Bild aus dem Alltag - Hund mit Sonnenbrille, Leute die in einem Brunnen baden</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
    <concept id="scn012400" modified="2010-12-15T22:14:09+00:00">
      <conceptId qcode="scn:012400" created="2008-01-01T01:00:00+00:00"/>
      <type qcode="cpnat:abstract"/>
      
      <name xml:lang="de">Filmszene</name>
      <definition xml:lang="de">Bilder wie während der Aufnahmen zu einem Film oder einer Fernsehproduktion gemacht wurden</definition>
      
      <related uri="http://cv.iptc.org/newscodes/scene/" rel="skos:inScheme"/>
    </concept>
  </conceptSet>
